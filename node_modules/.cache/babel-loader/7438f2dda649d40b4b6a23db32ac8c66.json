{"ast":null,"code":"import \"core-js/modules/es.symbol.js\";\nimport \"core-js/modules/es.symbol.description.js\";\nimport useVuelidate from \"@vuelidate/core\";\nimport { mapState } from \"vuex\";\nimport RegistrateDataSrvice from \"/src/services/RegistrateDataSrvice\";\nimport appConfig from \"../../../app.config\";\nexport default {\n  setup: function setup() {\n    return {\n      v$: useVuelidate()\n    };\n  },\n  page: {\n    title: \"Register\",\n    meta: [{\n      name: \"description\",\n      content: appConfig.description\n    }]\n  },\n  components: {},\n  data: function data() {\n    return {\n      User: {\n        id: \"\",\n        email: \"\",\n        password: \"\",\n        role: 1\n      },\n      submitted: false,\n      regError: null,\n      tryingToRegister: false,\n      isRegisterError: false,\n      registerSuccess: false\n    };\n  },\n  methods: {\n    addedUser: function addedUser() {\n      var _this = this;\n\n      var data = {\n        \"login\": this.User.login,\n        \"password\": this.User.password\n      };\n      console.log(data);\n      RegistrateDataSrvice.create(data).then(function (response) {\n        _this.User.id = response.data.id;\n        console.log(response.data);\n        _this.submitted = true;\n      }).catch(function (e) {\n        console.log(e);\n      });\n    },\n    User: function User() {\n      this.submitted = false;\n      this.User = {};\n    }\n  },\n  // Try to register the User in with the login, Username\n  // and password they provided.\n  tryToRegisterIn: function tryToRegisterIn() {\n    this.submitted = true; // stop here if form is invalid\n\n    this.v$.$touch();\n  }\n};","map":{"version":3,"mappings":";;AACA,OAAOA,YAAP,MAAyB,iBAAzB;AACA,SAASC,QAAT,QAAyB,MAAzB;AACA,OAAOC,oBAAP,MAAkC,oCAAlC;AACA,OAAOC,SAAP,MAAsB,qBAAtB;AAEA,eAAe;AACXC,OADW,mBACH;AACR,WAAO;AAAEC,QAAE,EAAEL,YAAY;AAAlB,KAAP;AACD,GAHY;AAIbM,MAAI,EAAE;AACJC,SAAK,EAAE,UADH;AAEJC,QAAI,EAAE,CACJ;AACEC,UAAI,EAAE,aADR;AAEEC,aAAO,