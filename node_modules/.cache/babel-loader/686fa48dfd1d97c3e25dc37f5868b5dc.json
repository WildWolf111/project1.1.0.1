 jsonFields: [{\r\n                name:'',\r\n                value: ''\r\n      }],\r\n        \r\n    };\r\n    \r\n  },\r\n  components: {\r\n    Layout,\r\n    PageHeader,\r\n    Select2\r\n  },\r\n    methods: {\r\n      //добавление новой строки\r\n       AddField: function () {\r\n         console.log('__________')\r\n         console.log(this.Document.Items)\r\n          this.submitted = false;\r\n          console.log(\"*********\")\r\n          \r\n      this.Document.Items.push({\r\n        Id:'',\r\n        TrustedSignId:'',\r\n        Product:{\r\n          Id:'',\r\n          Name:'',\r\n        } ,\r\n        Quantity:'',\r\n        Price:'',\r\n        GTD:{\r\n          Id:'',\r\n          NumId:''\r\n        },\r\n        Warehouse:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        Warehouse_cell:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        \r\n        });\r\n      \r\n    }, //добавление новой строки(конец)\r\n      \r\n      \r\n            //удаление строки\r\n      RemoveField: function(index){\r\n\r\n        this.Document.Items.splice(index,1);\r\nconsole.log(index)\r\n      },//удаление строки конец\r\n      \r\n\r\n     \r\n\r\n\r\n\r\n\r\n\r\n    AddIncome() {\r\n      console.log(\"<<<<<<<<<<>>>>>>>>>>>\")\r\n      console.log(this.Document.Items.Product);\r\n      var data = {\r\n      'id':this.Document.Id,\r\n    'CompanySender':{\r\n          'Id':this.Document.CompanySender.Id,\r\n          'Name':this.Document.CompanySender.Name\r\n      },\r\n    'CompanyRecipient':{\r\n          'Id':this.Document.CompanyRecipient.Id,\r\n          'Name':this.Document.CompanyRecipient.Name\r\n      },\r\n    'Items':[{\r\n        'id':this.Document.Items.Id,\r\n        'trusted_sign_id':this.Document.Items.TrustedSignId,\r\n        'Product':{\r\n          'Id':this.Document.Items.Product.Id,\r\n          'Name':this.Document.Items.Product.Name,\r\n        } ,\r\n        'quantity':this.Document.Items.Quan