duct = response.data.List[i];\r\n                console.log(i);\r\n               this.Products[i]={\r\n        \r\n                id:currentProduct.Id,\r\n                text:currentProduct.Name  \r\n\r\n              };\r\n }console.log(this.Products)\r\n          //this.Brands = response.data.Brands;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },//конец блока (end)\r\n\r\n\r\n\r\n\r\n//получение компаний\r\nretrieveCompanies() {\r\n         console.log(\"654365437653654254376254265\")\r\n         \r\n        this.jsonPages = {\r\n          pg_number:this.page,\r\n          pg_length: this.perPage\r\n      };\r\n\r\n      if (this.search!=\"\"){\r\n       this.jsonFields = [{\r\n          field:\"name\",\r\n          value: this.search\r\n       }];\r\n       }else{\r\n           this.jsonFields = [];\r\n       }\r\n\r\n\r\n       CompaniesDataService.getAll(this.jsonFields,[],this.jsonPages)\r\n        .then(response => {\r\n           console.log(\"--------COMPANIES------------\")\r\n           \r\n           \r\n          let i=0;\r\n          for(i=0; i<response.data.total_rec;i++){\r\n               let currentCompany = response.data.List[i];\r\n                console.log(i);\r\n               this.Companies[i]={\r\n        \r\n                id:currentCompany.id,\r\n                text:currentCompany.name  \r\n\r\n              };\r\n          }\r\n\r\n          console.log(this.Companies)\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n\r\n        \r\n    },//конец\r\n\r\n//получение складских ячеек\r\n    retrieveWarehouse_cells() {\r\n         console.log(\"*******************\")\r\n          console.log(this.page)\r\n        this.jsonPages = {\r\n          pg_number:this.page,\r\n          pg_length: this.perPage\r\n      };\r\n\r\n      if (this.search!=\"\"){\r\n       this.jsonFields = [{\r\n          field:\"name\",\r\n          value: this.search\r\n       }];\