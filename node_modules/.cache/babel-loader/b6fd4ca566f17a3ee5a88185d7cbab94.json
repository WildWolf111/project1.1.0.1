  var _this2 = this;\n\n      var plus = document.getElementsByClassName(\"plus\");\n      var minus = document.getElementsByClassName(\"minus\");\n      var product = document.getElementsByClassName(\"product\");\n\n      if (plus) {\n        Array.prototype.forEach.call(plus, function (e) {\n          e.addEventListener(\"click\", function (event) {\n            var par = event.target.closest(\".input-step\");\n            par.getElementsByClassName(\"product-quantity\")[0].value++;\n            Array.prototype.forEach.call(product, function () {\n              _this2.updateQuantity(event.target);\n            });\n          });\n        });\n      }\n\n      if (minus) {\n        Array.prototype.forEach.call(minus, function (e) {\n          e.addEventListener(\"click\", function (event) {\n            var par = event.target.closest(\".input-step\");\n            if (par.getElementsByClassName(\"product-quantity\")[0].value > 0) par.getElementsByClassName(\"product-quantity\")[0].value--;\n            Array.prototype.forEach.call(product, function () {\n              _this2.updateQuantity(event.target);\n            });\n          });\n        });\n      }\n    },\n    updateQuantity: function updateQuantity(quantityInput) {\n      var _this3 = this;\n\n      var productRow = quantityInput.closest(\".product\");\n      var price;\n      Array.prototype.forEach.call(productRow.getElementsByClassName(\"product-price\"), function (e) {\n        price = parseFloat(e.innerHTML);\n      });\n\n      if (quantityInput.previousElementSibling && quantityInput.previousElementSibling.classList.contains(\"product-quantity\")) {\n        this.quantity = quantityInput.previousElementSibling.value;\n      } else if (quantityInput.nextElementSibling && quantityInput.nextElementSibling.classList.contains(\"product-quantity\")) {\n        this.quantity = quantityInput.nextElementSibling.value;\n      }\n\n      var linePrice = price * this.quantity;\n      /* Update line price display and recalc cart totals */\n\n      Array.prototype.forEach.call(productRow.getElementsByClassName(\"product-line-price\"), function (e) {\n        e.innerHTML = linePrice.toFixed(2);\n\n        _this3.recalculateCart();\n      });\n    },\n    recalculateCart: function recalculateCart() {\n      var subtotal = 0;\n      Array.prototype.forEach.call(document.getElementsByClassName(\"product\"), function (item) {\n        Array.prototype.forEach.call(item.getElementsByClassName(\"product-line-price\"), function (e) {\n          subtotal += parseFloat(e.innerHTML);\n        });\n      });\n      /* Calculate totals */\n\n      var tax = subtotal * this.taxRate;\n      var discount = subtotal * this.discountRate;\n      var shipping = subtotal > 0 ? this.shippingRate : 0;\n      var total = subtotal + tax + shipping - discount;\n      document.getElementById(\"cart-subtotal\").innerHTML = this.currencySign + subtotal.toFixed(2);\n      document.getElementById(\"cart-tax\").innerHTML = this.currencySign + tax.toFixed(2);\n      document.getElementById(\"cart-shipping\").innerHTML = this.currencySign + shipping.toFixed(2);\n      document.getElementById(\"cart-total\").innerHTML = this.currencySign + total.toFixed(2);\n      document.getElementById(\"cart-discount\").innerHTML = \"-\" + this.currencySign + discount.toFixed(2);\n    }\n  }\n};","map":{"version":3,"mappings":";;;;AACA,OAAOA,MAAP,MAAmB,2BAAnB;AACA,OAAOC,SAAP,MAAsB,wBAAtB;AACA,OAAOC,UAAP,MAAuB,0BAAvB;AAEA,OAAOC,aAAP,MAA0B,oCAA1B;AACA,OAAOC,cAAP,MAA2B,oCAA3B;AAEA,OAAOC,MAAP,MAAmB,iCAAnB;AAEA,eAAe;AACbC,MAAI,EAAE;AACJC,SAAK,EAAE,eADH;AAEJC,QAAI,EAAE,CACJ;AACEC,UAAI,EAAE,aADR;AAEEC,aAAO,EAAET,SAAS,CAACU;AAFrB,KADI;AAFF,GADO;AAUbC,YAAU,EAAE;AACVZ,UAAM,EAANA,MADU;AAEVE,cAAU,EAAVA,UAFU;AAGVW,UAAM,EAAER;AAHE,GAVC;AAebS,MAfa,kBAeN;AACL,WAAO;AACLP,WAAK,EAAE,eADF;AAELQ,WAAK,EAAE,CACL;AACEC,YAAI,EAAE,WADR;AAEEC,YAAI,EAAE;AAFR,OADK,EAKL;AACED,YAAI,EAAE,eADR;AAEEE,cAAM,EAAE;AAFV,OALK,CAFF;AAYLC,aAAO,EAAE,KAZJ;AAaLC,kB