{\r\n          Id:'',\r\n      },\r\n    Items:[],\r\n    Comment:'',\r\n  \r\n    PriceType:{\r\n        Id:'',\r\n        Name:'',\r\n      },\r\n    DeadLine:'',\r\n    Sum:'',\r\n      },\r\n\r\n Items:[{\r\n        Id:'',\r\n        TrustedSignId:'',\r\n        Product:{\r\n          Id:'',\r\n        } ,\r\n        Quantity:'',\r\n        Price:'',\r\n        GTD:{\r\n          Id:'',\r\n        },\r\n        Warehouse:{\r\n          Id:'',\r\n        },\r\n        Warehouse_cell:{\r\n          Id:'',\r\n        },       \r\n        }]\r\n  ,\r\n  Companies:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  GTD:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Products:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Warehouses:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Warehouse_cells:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n\r\n\r\n\r\n    jsonPages: {\r\n                PageNumber: 1,\r\n                TotalRecords: 100\r\n      },\r\n       jsonFields: [{\r\n                name:'',\r\n                value: ''\r\n      }],\r\n        \r\n    };\r\n    \r\n  },\r\n  components: {\r\n    Layout,\r\n    PageHeader,\r\n    Select2\r\n  },\r\n    methods: {\r\n      //добавление новой строки\r\n       AddField: function () {\r\n         console.log('__________')\r\n         console.log(this.Document.Items)\r\n          this.submitted = false;\r\n          console.log(\"*********\")\r\n          \r\n      this.Document.Items.push({\r\n        Id:'',\r\n        TrustedSignId:'',\r\n        Product:{\r\n          Id:'',\r\n          Name:'',\r\n        } ,\r\n        Quantity:'',\r\n        Price:'',\r\n        GTD:{\r\n          Id:'',\r\n          NumId:''\r\n        },\r\n        Warehouse:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        Warehouse_cell:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        \r\n        });\r\n      \r\n    }, //добавление новой строки(конец)\r\n      \r\n      \r\n            //удаление строки\r\n      RemoveField: function(index){\r\n\r\n        this.Document.Items.splice(index,1);\r\nconsole.log(index)\r\n      },//удаление строки конец\r\n      \r\n\r\n     \r\n\r\n\r\n\r\n\r\n\r\n    AddIncome() {\r\n\r\n      let jsonItems = [];\r\n      let i=0;\r\n        for(i=0; i<this.Document.Items.length;i++){\r\n          let customItem = this.Document.Items[i];\r\n          console.log(customItem);\r\n         \r\n        jsonItems[i]={\r\n        'id':this.customItem.Id,\r\n        'trusted_sign_id':this.customItem.TrustedSignId,\r\n        'Product':{\r\n           'id':this.customItem.Product.Id,\r\n        } ,\r\n        'quantity':this.customItem.Quantity,\r\n        'price':this.customItem.Price,\r\n        'GTD':{\r\n          'id':this.customItem.GTD.Id,\r\n        },\r\n         'Warehouse':{\r\n           'id':this.customItem.Warehouse.Id,\r\n\r\n         },\r\n     'Warehouse_cells':{\r\n           'id':this.customItem.Warehouse_cells.Id,\r\n         }    \r\n        };\r\n        \r\n\r\n      }\r\n      console.log(jsonItems)\r\n\r\n      console.log(\"<<<<<<<<<<>>>>>>>>>>>\")\r\n      console.log(this.Document.Items.Product);\r\n      var data = {\r\n      'id':this.Document.Id,\r\n    'CompanySender':{\r\n          'Id':this.Document.CompanySender.Id,\r\n      },\r\n    'CompanyRecipient':{\r\n          'Id':this.Document.CompanyRecipient.Id,\r\n      },\r\n    'Items':this.jsonItems,\r\n    'comment':this.Document.Comment,\r\n  \r\n    'PriceType':{\r\n        'id':this.Document.PriceType.Id,\r\n      },\r\n    'DeadLine':this.Document.DeadLine,\r\n    'sum':this.Document.Sum,\r\n\r\n\r\n      };\r\n      \r\n   \r\n      DocumentsDataService.create(data)\r\n        .then(response => {\r\n   