\r\n                allpages: 0,\r\n                countsrecordonpage: 0,\r\n                allrecords: 0,\r\n                remainedrecords: 0\r\n            }\r\n        },\r\n      page: 1,\r\n      perPage: 7,\r\n      pages: [],\r\n      jsonPages: {\r\n                currentpage: 0,\r\n                countsrecordonpage: 0\r\n      },\r\n       jsonFields: [{\r\n                name:'',\r\n                value: ''\r\n      }],\r\n      search:\"\",\r\n    \r\n    }; \r\n       \r\n  },\r\n\r\n  components: {\r\n    Layout,\r\n    PageHeader,\r\n  },\r\n   methods: {\r\n    setPage(numPage){\r\n \r\n        this.page = numPage;\r\n         this.retrieveCategories();\r\n    },\r\n    retrieveCategories() {\r\n      this.jsonPages = {\r\n          currentpage:this.page,\r\n          countsrecordonpage: this.perPage\r\n      };\r\n\r\n      if (this.search!=\"\"){\r\n       this.jsonFields = [{\r\n          field:\"name\",\r\n          value: this.search\r\n       }];\r\n       }else{\r\n           this.jsonFields = [];\r\n       }\r\n\r\n\r\n       CategoriesDataService.getAll(this.jsonFields,[],this.jsonPages)\r\n        .then(response => {\r\n       \r\n          this.Categories = response.data.Items;\r\n           \r\n          this.perPage = response.data.Filter.Pages.countsrecordonpage;\r\n       \r\n          this.page = response.data.Filter.Pages.currentpage;\r\n          let i=0;\r\n          for(i=0; i<response.data.Filter.Pages.allpages;i++){\r\n              this.pages[i]=i+1;\r\n          }\r\n\r\n         \r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    refreshList() {\r\n      this.retrieveCategories();\r\n      this.currentCategories = null;\r\n      this.currentIndex = -1;\r\n    },\r\n\r\n    setActiveCategories(categories, index) {\r\n      this.currentCategories = categories;\r\n      this.currentIndex = categories ? index : -1;\r\n    },\r\n\r\n    removeAllCategories() {\r\n       CategoriesDataService.deleteAll()\r\n        .then(response => {\r\n       \r\n          this.refreshList();\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    \r\n\r\n    getCategoryByName(){\r\n         this.retrieveCategories();\r\n         this.pages = [];\r\n      \r\n    },\r\n\r\n     deleteCategory(id) {\r\n     \r\n      CategoriesDataService.delete(id)\r\n        .then(response => {\r\n        \r\n         \r\n        if (response.data.message = 202){\r\n        this.retrieveCategories();\r\n        }\r\n        else{\r\n        return response.data.message}\r\n        \r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n        \r\n    }\r\n  },\r\n  mounted() {\r\n    this.retrieveCategories();\r\n  },\r\n    \r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    \r\n    <div>\r\n    <h4>Categories</h4>\r\n    </div>\r\n\r\n<div>\r\n    <div class=\"col-lg-6\">\r\n         <div class=\"input-group\">\r\n                <input type=\"text\" class=\"form-control\" aria-label=\"Recipient's username\" aria-describedby=\"button-addon2\" id=\"search\" v-model=\"search\">\r\n                <button class=\"btn btn-outline-success\" type=\"button\" id=\"button-addon2\" @click=\"getCategoryByName\" key=\"enter\">search</button>\r\n        \r\n         </div>\r\n    </div>\r\n</div>\r\n<p></p>\r\n  \r\n     <table class=\"table table-primary table-striped align-middle table-nowrap mb-0\">\r\n    <thead>\r\n        <tr>\r\n            <th scope=\"col\">Id</th>\r\n            <th scope=\"col\">Name</th>\r\n            <th scope=\"col\">Slug</th>\r\n            <th scope=\"col\"></th>\r\n            \r\n            \r\n        \r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n        <tr v-for=\"(Category, index) in Categories\" :key=\"index\">\r\n    \r\n           \r\n            <td>{{ Category.id_categories }}</td>\r\n            <td>{{ Category.name }}</td>\r\n            <td>{{ Category.slug }}</td>\r\n            <td>\r\n                <div class=\"hstack gap-3 flex-wrap\">\r\n                   <router-link :to=\"{name: 'CategoriesEdit', params: { id: Category.id_categories }}\"> <a href=\"javascript:void(0);\" class=\"link-primary fs-15\"><i class=\"ri-edit-2-line\"></i></a></router-link>\r\n                    <a href=\"javascript:void(0);\" class=\"link-success fs-15\"><i class=\"ri-delete-bin-line\" @click=\"deleteCategory(Category.id_categories)\"></i></a>\r\n                </div>\r\n            </td>\r\n        </tr>\r\n       \r\n    </tbody>\r\n    <p></p>\r\n</table>\r\n\r\n<div class=\"pagination-wrap hstack gap-2\">\r\n              <a\r\n                class=\"page-item pagination-prev disabled\"\r\n                href=\"#\"\r\n                v-if=\"page != 1\"\r\n                @click=\"setPage((--page))\"\r\n              >\r\n                Previous\r\n              </a>\r\n              <ul class=\"pagination listjs-pagination mb-0\">\r\n                <li\r\n                 :class=\"{\r\n                              active: pageNumber == page,\r\n                              disabled: pageNumber == '...',\r\n                            }\"\r\n                  v-for=\"(pageNumber, index) in pages.slice(page - 1, page + 5)\"\r\n                  :key=\"index\"\r\n                  @click=\"setPage(page = pageNumber)\"\r\n                >\r\n                  <a class=\"page\" href=\"#\"  @click=\"setPage(pageNumber)\">{{ pageNumber }}</a>\r\n                </li>\r\n              </ul>\r\n              <a\r\n                class=\"page-item pagination-next\"\r\n                href=\"#\"\r\n                @click=\"setPage(++page)\"\r\n                v-if=\"page < pages.length\"\r\n              >\r\n                Next\r\n              </a>\r\n            </div>\r\n\r\n\r\n\r\n\r\n    </Layout>\r\n</template>\r\n"]},"metadata":{},"sourceType":"module"}