ucts: null,\n      currentIndex: -1,\n      title: \"\",\n\n \n        Filter: {\n            Fields: [{\n                field:'',\n                value:''\n            }],\n            Sorts: [],\n            Pages: {\n                currentpage: 0,\n                allpages: 0,\n                countsrecordonpage: 0,\n                allrecords: 0,\n                remainedrecords: 0\n            }\n        },\n      page: 1,\n      perPage: 7,\n      pages: [],\n      jsonPages: {\n                currentpage: 0,\n                countsrecordonpage: 0\n      },\n       jsonFields: [{\n                name:'',\n                value: ''\n      }],\n      search:\"\",\n    \n    }; \n       \n  },\n\n  components: {\n    Layout,\n    PageHeader,\n  },\n   methods: {\n    setPage(numPage){\n \n        this.page = numPage;\n         this.retrieveProducts();\n    },\n    retrieveProducts() {\n      this.jsonPages = {\n          currentpage:this.page,\n          countsrecordonpage: this.perPage\n      };\n        console.log( '-----------')\n      console.log( this.jsonPages)\n      if (this.search!=\"\"){\n       this.jsonFields = [{\n          field:\"name\",\n          value: this.search\n       }];\n       }else{\n           this.jsonFields = [];\n       }\n\n\n       ProductDataService.getAll(this.jsonFields,[],this.jsonPages)\n        .then(response => {\n          this.Products = response.data.Items;\n          this.perPage = response.data.Filter.Pages.countsrecordonpage;\n          this.page = response.data.Filter.Pages.currentpage;\n          let i=0;\n          for(i=0; i<response.data.Filter.Pages.allpages;i++){\n              this.pages[i]=i+1;\n          }\n\n          console.log( this.Products);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n\n    refreshList() {\n      this.retrieveProducts();\n      this.currentProducts = null;\n      this.currentIndex = -1;\n    },\n\n    setActiveProducts(products, index) {\n      this.currentProducts = products;\n      this.currentIndex = products ? index : -1;\n    },\n\n    removeAllProducts() {\n       ProductDataService.deleteAll()\n        .then(response => {\n          console.log(response.data);\n          this.refreshList();\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n    \n\n    getProductByName(){\n         this.retrieveProducts();\n         this.pages = [];\n\n    },\n\n     deleteProduct(id) {\n     \n      ProductDataService.delete(id)\n        .then(response => {\n          console.log(response.data);\n         \n        if (response.data.message = 202){\n        this.retrieveProducts();\n        }\n        else{\n        return response.data.message}\n        \n        })\n        .catch(e => {\n          console.log(e);\n        });\n        \n    }\n  },\n  mounted() {\n    this.retrieveProducts();\n  },\n    \n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    \n    <div>\n    <h4>Products</h4>\n    </div>\n\n<div>\n    <div class=\"col-lg-6\">\n         <div class=\"input-group\">\n                <input type=\"text\" class=\"form-control\" aria-label=\"Recipient's username\" aria-describedby=\"button-addon2\" id=\"search\" v-model=\"search\">\n                <button class=\"btn btn-outline-success\" type=\"button\" id=\"button-addon2\" @click=\"getProductByName\" key=\"enter\">search</button>\n        \n         </div>\n    </div>\n</div>\n<p></p>\n  \n     <table class=\"table table-primary table-striped align-middle table-nowrap mb-0\">\n    <thead>\n        <tr>\n            <th scope=\"col\">Id</th>\n            <th scope=\"col\">Name</th>\n            <th scope=\"col\">Slug</th>\n            <th scope=\"col\"> SKU</th>\n            <th scope=\"col\">short_description</th>\n             <th scope=\"col\">Brand</th>\n            <th scope=\"col\"></th>\n            \n            \n            \n        \n        </tr>\n    </thead>\n    <tbody>\n        <tr v-for=\"(Product, index) in Products\" :key=\"index\">\n    \n           \n            <td>{{ Product.Id }}</td>\n            <td>{{ Product.Name }}</td>\n            <td>{{ Product.Slug }}</td>\n            <td>{{ Product.SKU }}</td>\n            <td>{{ Product.short_description }}</td>\n            <td>{{ Product.Brand.Name }}</td>\n            <td>\n                <div class=\"hstack gap-3 flex-wrap\">\n                   <router-link :to=\"{name: 'ProductEdit', params: { id: Product.Id }}\"> <a href=\"javascript:void(0);\" class=\"link-primary fs-15\"><i class=\"ri-edit-2-line\"></i></a></router-link>\n                    <a href=\"javascript:void(0);\" class=\"link-success fs-15\"><i class=\"ri-delete-bin-line\" @click=\"deleteProduct(Product.Id)\"></i></a>\n                    <a href=\"javascript:void(0);\" class=\"link-success fs-15\"><i class=\"ri-delete-bin-line\" @click=\"deleteProduct(Product.Id)\"></i></a>\n                </div>\n            </td>\n        </tr>\n       \n    </tbody>\n    <p></p>\n</table>\n\n<div class=\"pagination-wrap hstack gap-2\">\n              <a\n                class=\"page-item pagination-prev disabled\"\n                href=\"#\"\n                v-if=\"page != 1\"\n                @click=\"setPage((--page))\"\n              >\n                Previous\n              </a>\n              <ul class=\"pagination listjs-pagination mb-0\">\n                <li\n                 :class=\"{\n                              active: pageNumber == page,\n                              disabled: pageNumber == '...',\n                            }\"\n                  v-for=\"(pageNumber, index) in pages.slice(page - 1, page + 5)\"\n                  :key=\"index\"\n                  @click=\"setPage(page = pageNumber)\"\n                >\n                  <a class=\"page\" href=\"#\"  @click=\"setPage(pageNumber)\">{{ pageNumber }}</a>\n                </li>\n              </ul>\n              <a\n                class=\"page-item pagination-next\"\n                href=\"#\"\n                @click=\"setPage(++page)\"\n                v-if=\"page < pages.length\"\n              >\n                Next\n              </a>\n            </div>\n\n\n\n\n    </Layout>\n</template>\n"]},"metadata":{},"sourceType":"module"}