in","password","submitted","authError","tryingToLogIn","isAuthError","validations","withMessage","computed","notification","$store","state","methods","loginF","dispatch","commit","console","log","userService","then","user","router","push","error","root","logout","registeruser","register","tryToLogIn","$touch","$invalid"],"sourceRoot":"","sources":["C:\\Projects\\success\\project_v.1.0.0\\src\\views\\account\\login.vue"],"sourcesContent":["<script>\nimport { mapState } from \"vuex\";\nimport { required,  helpers } from \"@vuelidate/validators\";\nimport useVuelidate from \"@vuelidate/core\";\nimport appConfig from \"../../../app.config\";\n\nimport {\n  \n  authFackMethods,\n  notificationMethods,\n} from \"@/state/helpers\";\n\nexport default {\n  setup() {\n    return { v$: useVuelidate() };\n  },\n  page: {\n    title: \"Login\",\n    meta: [\n      {\n        name: \"description\",\n        content: appConfig.description,\n      },\n    ],\n  },\n  data() {\n    return {\n      login: \"\",\n      password: \"\",\n      submitted: false,\n      authError: null,\n      tryingToLogIn: false,\n      isAuthError: false,\n    };\n  },\n  validations: {\n    login: {\n      required: helpers.withMessage(\"login is required\", required),\n    \n    },\n    password: {\n      required: helpers.withMessage(\"Password is required\", required),\n    },\n  },\n  computed: {\n    ...mapState(\"authfack\", [\"status\"]),\n    notification() {\n      return this.$store ? this.$store.state.notification : null;\n    },\n  },\n  methods: {\n    ...notificationMethods,\n    loginF({ dispatch, commit }, { login, password }) {\n        console.log(login, password)\n\n\n        userService.login(login, password)\n            .then(\n\n                user => {\n                    commit('loginSuccess', user);\n                    router.push('/');\n                },\n                error => {\n                    commit('loginFailure', error);\n                    dispatch('notification/error', error, { root: true });\n                }\n            );\n    },\n    // Logout the user\n    logout({ commit }) {\n        userService.logout();\n        commit('logout');\n    },\n    // register the user\n    registeruser({ dispatch, commit }, user) {\n        commit('registerRequest', user);\n        userService.register(user)\n            .then(\n                user => {\n                    commit('registerSuccess', user);\n                    dispatch('notification/success', 'Registration successful', { root: true });\n                    router.push('/login');\n                },\n                error => {\n                    commit('registerFailure', error);\n                    dispatch('notification/error', error, { root: true });\n                }\n            );\n    },\n    \n    // Try to log the user in with the username\n    // and password they provided.\n    tryToLogIn() {\n      this.submitted = true;\n      // stop here if form is invalid\n      this.v$.$touch();\n     \n      if (this.v$.$invalid) {\n        return;\n      } else {\n           const { login, password } = this;\n          console.log(login, password)\n          if (login && password) {\n            this.loginF({\n              login,\n              password,\n            });\n          }\n      }\n    },\n  },\n};\n</script>\n\n<template>\n  <div class=\"auth-page-wrapper pt-5\">\n    <!-- auth page bg -->\n    <div class=\"auth-one-bg-position auth-one-bg\" id=\"auth-particles\">\n      <div class=\"bg-overlay\"></div>\n\n      <div class=\"shape\">\n        <svg\n          xmlns=\"http://www.w3.org/2000/svg\"\n          version=\"1.1\"\n          xmlns:xlink=\"http://www.w3.org/1999/xlink\"\n          viewBox=\"0 0 1440 120\"\n        >\n          <path\n            d=\"M 0,36 C 144,53.6 432,123.2 720,124 C 1008,124.8 1296,56.8 1440,40L1440 140L0 140z\"\n          ></path>\n        </svg>\n      </div>\n    </div>\n\n    <!-- auth page content -->\n    <div class=\"auth-page-content\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-lg-12\">\n            <div class=\"text-center mt-sm-5 mb-4 text-white-50\">\n              <div>\n                <router-link to=\"/\" class=\"d-inline-block auth-logo\">\n                  <img\n                    src=\"@/assets/images/logo-light.png\"\n                    alt=\"\"\n                    height=\"20\"\n                  />\n                </router-link>\n              </div>\n              <p class=\"mt-3 fs-15 fw-medium\">\n                Premium Admin & Dashboard Template\n              </p>\n            </div>\n          </div>\n        </div>\n        <!-- end row -->\n\n        <div class=\"row justify-content-center\">\n          <div class=\"col-md-8 col-lg-6 col-xl-5\">\n            <div class=\"card mt-4\">\n              <div class=\"card-body p-4\">\n                <div class=\"text-center mt-2\">\n                  <h5 class=\"text-primary\">Welcome Back !</h5>\n                  <p class=\"text-muted\">Sign in to continue to Velzon.</p>\n                </div>\n                <div class=\"p-2 mt-4\">\n                  <b-alert\n                    v-model=\"isAuthError\"\n                    variant=\"danger\"\n                    class=\"mt-3\"\n                    dismissible\n                    >{{ authError }}</b-alert\n                  >\n\n                  <div\n                    v-if=\"notification.message\"\n                    :class=\"'alert ' + notification.type\"\n                  >\n                    {{ notification.message }}\n                  </div>\n\n                  <form @submit.prevent=\"tryToLogIn\">\n                    <div class=\"mb-3\">\n                      <label for=\"login\" class=\"form-label\">login</label>\n                      <input\n                        type=\"login\"\n                        class=\"form-control\"\n                        id=\"login\"\n                        placeholder=\"Enter login\"\n                        v-model=\"login\"\n                        :class=\"{\n                          'is-invalid': submitted && v$.login.$error,\n                        }\"\n                      />\n                      <div\n                        v-for=\"(item, index) in v$.login.$errors\"\n                        :key=\"index\"\n                        class=\"invalid-feedback\"\n                      >\n                        <span v-if=\"item.$message\">{{ item.$message }}</span>\n                      </div>\n                    </div>\n\n                    <div class=\"mb-3\">\n                      <div class=\"float-end\">\n                        <router-link\n                          to=\"/forgot-password\"\n                          class=\"text-muted\"\n                          >Forgot password?</router-link\n                        >\n                      </div>\n                      <label class=\"form-label\" for=\"password-input\"\n                        >Password</label\n                      >\n                      <div class=\"position-relative auth-pass-inputgroup mb-3\">\n                        <input\n                          type=\"password\"\n                          v-model=\"password\"\n                          class=\"form-control pe-5\"\n                          :class=\"{\n                            'is-invalid': submitted && v$.password.$error,\n                          }\"\n                          placeholder=\"Enter password\"\n                          id=\"password-input\"\n                        />\n                        <button\n                          class=\"\n                            btn btn-link\n                            position-absolute\n                            end-0\n                            top-0\n                            text-decoration-none text-muted\n                          \"\n                          type=\"button\"\n                          id=\"password-addon\"\n                        >\n                          <i class=\"ri-eye-fill align-middle\"></i>\n                        </button>\n                        <div\n                          v-if=\"submitted && v$.password.$error\"\n                          class=\"invalid-feedback\"\n                        >\n                          <span v-if=\"v$.password.required.$message\">{{\n                            v$.password.required.$message\n                          }}</span>\n                        </div>\n                      </div>\n                    </div>\n\n                    <div class=\"form-check\">\n                      <input\n                        class=\"form-check-input\"\n                        type=\"checkbox\"\n                        value=\"\"\n                        id=\"auth-remember-check\"\n                      />\n                      <label class=\"form-check-label\" for=\"auth-remember-check\"\n                        >Remember me</label\n                      >\n                    </div>\n\n                    <div class=\"mt-4\">\n                      <button class=\"btn btn-success w-100\" type=\"submit\">\n                        Sign In\n                      </button>\n                    </div>\n\n                    <div class=\"mt-4 text-center\">\n                      <div class=\"signin-other-title\">\n                        <h5 class=\"fs-13 mb-4 title\">Sign In with</h5>\n                      </div>\n                      <div>\n                        <button\n                          type=\"button\"\n                          class=\"\n                            btn btn-primary btn-icon\n                            waves-effect waves-light\n                          \"\n                        >\n                          <i class=\"ri-facebook-fill fs-16\"></i>\n                        </button>\n                        <button\n                          type=\"button\"\n                          class=\"\n                            btn btn-danger btn-icon\n                            waves-effect waves-light\n                            ms-1\n                          \"\n                        >\n                          <i class=\"ri-google-fill fs-16\"></i>\n                        </button>\n                        <button\n                          type=\"button\"\n                          class=\"\n                            btn btn-dark btn-icon\n                            waves-effect waves-light\n                            ms-1\n                          \"\n                        >\n                          <i class=\"ri-github-fill fs-16\"></i>\n                        </button>\n                        <button\n                          type=\"button\"\n                          class=\"\n                            btn btn-info btn-icon\n                            waves-effect waves-light\n                            ms-1\n                          \"\n                        >\n                          <i class=\"ri-twitter-fill fs-16\"></i>\n                        </button>\n                      </div>\n                    </div>\n                  </form>\n                </div>\n              </div>\n              <!-- end card body -->\n            </div>\n            <!-- end card -->\n\n            <div class=\"mt-4 text-center\">\n              <p class=\"mb-0\">\n                Don't have an account ?\n                <router-link\n                  to=\"/register\"\n                  class=\"fw-semibold text-primary text-decoration-underline\"\n                >\n                  Signup\n                </router-link>\n              </p>\n            </div>\n          </div>\n        </div>\n        <!-- end row -->\n      </div>\n      <!-- end container -->\n    </div>\n    <!-- end auth page content -->\n\n    <!-- footer -->\n    <footer class=\"footer\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-lg-12\">\n            <div class=\"text-center\">\n              <p class=\"mb-0 text-muted\">\n                &copy; {{ new Date().getFullYear() }} Velzon. Crafted with\n                <i class=\"mdi mdi-heart text-danger\"></i> by Themesbrand\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </footer>\n    <!-- end Footer -->\n  </div>\n  <!-- end auth-page-wrapper -->\n</template>"]},"metadata":{},"sourceType":"module"}