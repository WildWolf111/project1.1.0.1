>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"columnDatalabelChart.series\"\n              :options=\"columnDatalabelChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Stacked Column Charts</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"stackedColumnsChart.series\"\n              :options=\"stackedColumnsChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Stacked Column 100</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"columnStackedChart.series\"\n              :options=\"columnStackedChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Column with Markers</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"columnMarkersChart.series\"\n              :options=\"columnMarkersChart.chartOptions\"\n            ></apexchart>\n            <div id=\"column_markers\" class=\"apex-charts\" dir=\"ltr\"></div>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Column with Rotated Labels</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"columnRotatedLabel.series\"\n              :options=\"columnRotatedLabel.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Column with Nagetive Values</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"columnNegativeChart.series\"\n              :options=\"columnNegativeChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Range Column Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"rangeColumnChart.series\"\n              :options=\"rangeColumnChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Dynamic Loaded Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <div id=\"dynamicloadedchart-wrap\">\n              <div id=\"chart-year\">\n                <apexchart\n                  type=\"bar\"\n                  height=\