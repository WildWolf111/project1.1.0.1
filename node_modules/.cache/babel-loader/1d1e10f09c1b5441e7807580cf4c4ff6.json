 '',last: '' }],\r\n      title: \"Добавление нового документа\",\r\n      \r\n      \r\n      \r\n      \r\n      Document:{\r\n    Id:'',\r\n    CompanySender:{\r\n          Id:'',\r\n          Name:'',\r\n      },\r\n    CompanyRecipient:{\r\n          Id:'',\r\n          Name:'',\r\n      },\r\n    Items:[{\r\n        Id:'',\r\n        TrustedSignId:'',\r\n        Product:{\r\n          Id:'',\r\n          Name:'',\r\n        } ,\r\n        Quantity:'',\r\n        Price:'',\r\n        GTD:{\r\n          Id:'',\r\n          NumId:''\r\n        },\r\n        Warehouse:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        Warehouse_cell:{\r\n          Id:'',\r\n          Name:'',\r\n        },       \r\n        }],\r\n    Comment:'',\r\n  \r\n    PriceType:{\r\n        Id:'',\r\n        Name:'',\r\n      },\r\n    DeadLine:'',\r\n    Sum:'',\r\n      },\r\n\r\n\r\n  Companies:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  GTD:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Products:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Warehouses:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Warehouse_cells:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n\r\n\r\n\r\n    jsonPages: {\r\n                PageNumber: 1,\r\n                TotalRecords: 100\r\n      },\r\n       jsonFields: [{\r\n                name:'',\r\n                value: ''\r\n      }],\r\n        \r\n    };\r\n    \r\n  },\r\n  components: {\r\n    Layout,\r\n    PageHeader,\r\n    Select2\r\n  },\r\n    methods: {\r\n      //добавление новой строки\r\n       AddField: function () {\r\n         console.log('__________')\r\n         console.log(this.Document.Items)\r\n          this.submitted = false;\r\n          console.log(\"*********\")\r\n          \r\n      this.Document.Items.push({\r\n        Id:'',\r\n        TrustedSignId:'',\r\n        Product:{\r\n          Id:'',\r\n          Name:'',\r\n        } ,\r\n        Quantity:'',\r\n        Price:'',\r\n        GTD:{\r\n          Id:'',\r\n          NumId:''\r\n        },\r\n        Warehouse:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        Warehouse_cell:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        \r\n        });\r\n      \r\n    }, //добавление новой строки(конец)\r\n      \r\n      \r\n            //удаление строки\r\n      RemoveField: function(index){\r\n\r\n        this.Document.Items.splice(index,1);\r\nconsole.log(index)\r\n      },//удаление строки конец\r\n      \r\n\r\n     \r\n\r\n\r\n\r\n\r\n\r\n\r\n    addedIncome_Doc() {\r\n      var data = {\r\n        \r\n      'id':this.Document.Id,\r\n    'CompanySender':{\r\n          'Id':this.Document.Company.Id,\r\n          'Name':this.Document.Company.Name\r\n      },\r\n    'CompanyRecipient':{\r\n          'Id':this.Document.Company.Id,\r\n          'Name':this.Document.Company.Name\r\n      },\r\n    'Items':[{\r\n        'id':this.Document.Items.Id,\r\n        'trusted_sign_id':this.Document.Items.TrustedSignId,\r\n        'Product':{\r\n          'id':this.Document.Items.Product.Id,\r\n          'name':this.Document.Items.Product.Name,\r\n        } ,\r\n        'quantity':this.Document.Items.Quantity,\r\n        'price':this.Document.Items.Price,\r\n        'GTD':{\r\n          'price':this.Document.Items.GTD.Id,\r\n          'num_id':this.Document.Items.GTD.NumId\r\n        },\r\n         'Warehouse':{\r\n           \"id\":this.Document.Items.Warehouse.Id,\r\n           \"name\":this.Document.Items.Warehouse.Name\r\n\r\n         },\r\n     'Warehouse_cells':{\r\n           \"id\":this.Document.Items.Warehouse_cells.Id,\r\n           \"name\":this.Document.Items.Warehouse_cells.Name\r\n\r\n         },     \r\n        }],\r\n    'comment':