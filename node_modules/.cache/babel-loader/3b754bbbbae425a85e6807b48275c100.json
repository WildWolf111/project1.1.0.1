"PageHeader","appConfig","Select2","ProductsDataService","DocumentsDataService","CompaniesDataService","Warehouse_cellDataService","jsonpage","title","meta","name","content","description","data","fields","first","last","Document","Id","CompanySender","Name","CompanyRecipient","Items","TrustedSignId","Product","Quantity","Price","GTD","NumId","Warehouse","Warehouse_cell","Comment","PriceType","DeadLine","Sum","Companies","id","text","Products","Warehouses","Warehouse_cells","jsonPages","PageNumber","TotalRecords","jsonFields","value","components","methods","AddField","console","log","Item","submitted","push","RemoveField","index","splice","addedIncome_Doc","create","then","response","catch","e","newDocument","retrieveProducts","pg_number","pg_length","search","field","getAll","List","perPage","page","i","length","currentProduct","retrieveCompanies","total_pg","pages","retrieveWarehouse_cells","mounted"],"sourceRoot":"","sources":["C:\\Projects\\success\\project_v.1.0.0\\src\\views\\documents\\income\\add.vue"],"sourcesContent":["<script>\r\nimport Layout from \"../../../layouts/main.vue\";\r\nimport PageHeader from \"@/components/page-header\";\r\nimport appConfig from \"../../../../app.config\";\r\nimport Select2 from 'vue3-select2-component';\r\nimport ProductsDataService from \"/src/services/ProductsDataService\";\r\nimport DocumentsDataService from \"/src/services/DocumentsDataService\";\r\nimport CompaniesDataService from \"/src/services/CompaniesDataService\";\r\nimport Warehouse_cellDataService from \"/src/services/Warehouse_cellDataService\";\r\n\r\nexport default {\r\n  jsonpage: {\r\n    title: \"Basic Elements\",\r\n    meta: [{ name: \"description\", content: appConfig.description }],\r\n  },\r\n  data() {\r\n    \r\n    return {\r\n      fields: [{ first: '',last: '' }],\r\n      title: \"Добавление нового документа\",\r\n      \r\n      \r\n      \r\n      \r\n      Document:{\r\n    Id:'',\r\n    CompanySender:{\r\n          Id:'',\r\n          Name:''\r\n      },\r\n    CompanyRecipient:{\r\n          Id:'',\r\n          Name:''\r\n      },\r\n    Items:[{\r\n        Id:'',\r\n        TrustedSignId:'',\r\n        Product:{\r\n          Id:'',\r\n          Name:'',\r\n        } ,\r\n        Quantity:'',\r\n        Price:'',\r\n        GTD:{\r\n          Id:'',\r\n          NumId:''\r\n        },\r\n        Warehouse:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        Warehouse_cell:{\r\n          Id:'',\r\n          Name:'',\r\n        },       \r\n        }],\r\n    Comment:'',\r\n  \r\n    PriceType:{\r\n        Id:'',\r\n        Name:'',\r\n      },\r\n    DeadLine:'',\r\n    Sum:'',\r\n      },\r\n\r\n  Companies:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  GTD:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Products:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Warehouses:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n  Warehouse_cells:[\r\n          {\r\n            id:-1,\r\n            text:\"\"\r\n          }\r\n        ],\r\n\r\n\r\n\r\n    jsonPages: {\r\n          PageNumber: 0,\r\n          TotalRecords:0,\r\n      },\r\n    jsonFields: [{\r\n          name:'',\r\n          value: ''\r\n      }],\r\n        \r\n    };\r\n    \r\n  },\r\n  components: {\r\n    Layout,\r\n    PageHeader,\r\n    Select2\r\n  },\r\n    methods: {\r\n      //добавление новой строки\r\n       AddField: function () {\r\n         console.log('__________')\r\n         console.log(this.Document.Item)\r\n          this.submitted = false;\r\n          console.log(\"*********\")\r\n          \r\n      this.Document.Items.push({\r\n          Id:'',\r\n        TrustedSignId:'',\r\n         Product:{\r\n          Id:'',\r\n          Name:'',\r\n        } ,\r\n        Quantity:'',\r\n        Price:'',\r\n        GTD:{\r\n          Id:'',\r\n          NumId:''\r\n        },\r\n        Warehouse_cell:{\r\n          Id:'',\r\n          Name:'',\r\n        },\r\n        \r\n        });\r\n      \r\n    }, //добавление новой строки(конец)\r\n      \r\n      \r\n            //удаление строки\r\n      RemoveField: function(index){\r\n\r\n        this.Document.Items.splice(index,1);\r\nconsole.log(index)\r\n      },//удаление строки конец\r\n      \r\n\r\n     \r\n\r\n\r\n\r\n\r\n\r\n\r\n    addedIncome_Doc() {\r\n      var data = {\r\n        \r\n      'Id':this.Document.Id,\r\n    'CompanySender':{\r\n          'Id':this.Document.CompanySender.Id,\r\n          'Name':this.Document.CompanySender.Name\r\n      },\r\n    'CompanyRecipient':{\r\n          'Id':this.Document.CompanyRecipient.Id,\r\n          'Name':this.Document.CompanyRecipient.Name\r\n      },\r\n    'Items':[{\r\n        'Id':this.Document.Items.Id,\r\n        'TrustedSignId':this.Document.Items.TrustedSignId,\r\n        'Product':{\r\n          'Id':this.Document.Items.Product.Id,\r\n          'Name':this.Document.Items.Product.Name,\r\n        } ,\r\n        'Quantity':this.Document.Items.Quantity,\r\n        'Price':this.Document.Items.Price,\r\n        'GTD':{\r\n          'Id':this.Document.Items.GTD.Id,\r\n          'NumId':this.Document.Items.GTD.NumId\r\n        },\r\n         'Warehouse':{\r\n           \"id\":this.Document.Items.Warehouse.Id,\r\n           \"name\":this.Document.Items.Warehouse.Id\r\n\r\n         },\r\n     'Warehouse_cells':{\r\n           \"id\":this.Document.Items.Warehouse_cells.Id,\r\n           \"name\":this.Document.Items.Warehouse_cells.Name\r\n\r\n         },     \r\n        }],\r\n    'Comment':this.Document.Comment,\r\n  \r\n    'PriceType':{\r\n        'Id':this.Document.PriceType.Id,\r\n        'Name':this.Document.PriceType.Name,\r\n      },\r\n    'DeadLine':this.Document.DeadLine,\r\n    'Sum':this.Document.Sum,\r\n\r\n\r\n\r\n      };\r\n       console.log(data);\r\n      DocumentsDataService.create(data)\r\n        .then(response => {\r\n          this.Document.Id = response.data.id;\r\n          console.log(response.data);\r\n          this.submitted = true;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    \r\n    newDocument() {\r\n      this.submitted = false;\r\n      this.Document = {};\r\n    },\r\n\r\n\r\n     //полчение продукта (start)\r\n        retrieveProducts(){\r\n      this.jsonPages = {\r\n               pg_number:1,\r\n                  pg_length: 1000\r\n            };\r\n         \r\n            if (this.search!=\"\"){\r\n            this.jsonFields = [{\r\n                field:\"name\",\r\n                value: this.search\r\n            }];\r\n            }else{\r\n                this.jsonFields = [];\r\n            }\r\n\r\n\r\n\r\n            ProductsDataService.getAll(this.jsonFields,[],this.jsonPages)\r\n        .then(response => {\r\n\r\n\r\n          console.log(\"---------------------\")\r\n          console.log(response.data.List)\r\n\r\n\r\n          this.perPage = response.data.pg_length;\r\n\r\n          this.page = response.data.pg_number; \r\n\r\n           for(let i = 0; i < response.data.List.length; i++){\r\n                    \r\n              let currentProduct = response.data.List[i];           \r\n              \r\n              this.Products[i]={    \r\n\r\n                id:currentProduct.Id,\r\n\r\n                text:currentProduct.Name\r\n\r\n              };\r\n }\r\n          //this.Brands = response.data.Brands;\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },//конец блока (end)\r\n\r\n//получение компаний\r\nretrieveCompanies() {\r\n         console.log(\"*******************\")\r\n          console.log(this.page)\r\n        this.jsonPages = {\r\n          pg_number:this.page,\r\n          pg_length: this.perPage\r\n      };\r\n\r\n      if (this.search!=\"\"){\r\n       this.jsonFields = [{\r\n          field:\"name\",\r\n          value: this.search\r\n       }];\r\n       }else{\r\n           this.jsonFields = [];\r\n       }\r\n\r\n\r\n       CompaniesDataService.getAll(this.jsonFields,[],this.jsonPages)\r\n        .then(response => {\r\n           console.log(\"---------------------\")\r\n          console.log(response.data.List)\r\n          this.Companies = response.data.List;\r\n           \r\n          this.perPage = response.data.pg_length;\r\n          \r\n          this.page = response.data.pg_number;\r\n          \r\n          let i=0;\r\n          for(i=0; i<response.data.total_pg;i++){\r\n              this.pages[i]=i+1;\r\n              console.log(response.data.pg_length)\r\n              console.log(response.data.total_pg)\r\n          }\r\n\r\n         \r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },//конец\r\n\r\n//получение складских ячеек\r\n retrieveWarehouse_cells() {\r\n         console.log(\"*******************\")\r\n          console.log(this.page)\r\n        this.jsonPages = {\r\n          pg_number:this.page,\r\n          pg_length: this.perPage\r\n      };\r\n\r\n      if (this.search!=\"\"){\r\n       this.jsonFields = [{\r\n          field:\"name\",\r\n          value: this.search\r\n       }];\r\n       }else{\r\n           this.jsonFields = [];\r\n       }\r\n\r\n\r\n       Warehouse_cellDataService.getAll(this.jsonFields,[],this.jsonPages)\r\n        .then(response => {\r\n          console.log(\"---------------------\")\r\n          console.log(response.data.List)\r\n          this.Warehouse_cells = response.data.List;\r\n           \r\n          this.perPage = response.data.pg_length;\r\n          \r\n          this.page = response.data.pg_number;\r\n          \r\n          let i=0;\r\n          for(i=0; i<response.data.total_pg;i++){\r\n              this.pages[i]=i+1;\r\n           \r\n          }\r\n\r\n         \r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n\r\n\r\n  },\r\n   mounted() {\r\n  \r\n  },\r\n    \r\n};\r\n</script>\r\n\r\n<template>\r\n  <Layout>\r\n    <PageHeader :title=\"title\" :items=\"items\" />\r\n    \r\n      <div class=\"live-preview\">\r\n                            <form class=\"row g-3 needs-validation\" novalidate>\r\n                                <div class=\"col-md-4 position-relative\">\r\n                                <label for=\"exampleDataList\" class=\"form-label\" >От кого</label>\r\n                                <Select2 v-model=\"Document.CompanySender\" :key=\"Companies\" />\r\n                            </div>\r\n\r\n\r\n\r\n                                <div class=\"col-md-4 position-relative\">\r\n                                    <label for=\"exampleDataList\" class=\"form-label\" >Кому</label>\r\n                                    <Select2 v-model=\"Document.CompanyRecipient\" :key=\"Companies\" />\r\n                                </div>\r\n\r\n\r\n\r\n                                <div class=\"col-md-4\">\r\n                                    <label  class=\"form-label\">Тип документа</label>\r\n                                    <Select2 />\r\n                                </div>\r\n\r\n\r\n\r\n                                <div class=\"col-md-6 position-relative\">\r\n                                    <label for=\"validationTooltip03\" class=\"form-label\">Коментарий</label>\r\n                                    <input type=\"text\" class=\"form-control\" id=\"validationTooltip03\" required>\r\n                                   \r\n                                </div>\r\n\r\n                               \r\n\r\n                            <div class=\"col-xxl-3 col-md-6\">\r\n                                    <div>\r\n                                        <label for=\"exampleInputdate\" class=\"form-label\">Дата исполнения</label>\r\n                                        <input type=\"date\" class=\"form-control\" id=\"exampleInputdate\" v-model=\"Document.DeadLine\">\r\n                                    </div>\r\n                            </div>\r\n\r\n\r\n\r\n\r\n                              \r\n<p>\r\n \r\n    </p>\r\n\r\n\r\n\r\n\r\n\r\n <div>\r\n\r\n   <table class=\"table table-primary table-striped align-middle table-nowrap mb-0\">\r\n    <thead>\r\n        <tr>\r\n        \r\n            <th scope=\"row2\">Product</th>\r\n            <th scope=\"row2\">Warehouse</th>\r\n            <th scope=\"row2\">Warehouse_cell</th>\r\n            <th scope=\"row2\"> GTD</th>\r\n            <th scope=\"row2\">Quantity</th>\r\n            <th scope=\"row2\">Price</th>\r\n            <th scope=\"row2\">TrustedSignId</th>\r\n            <th scope=\"row2\"></th>\r\n            \r\n            \r\n            \r\n        \r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n        <tr v-for=\"(Item, index) in Document.Items\" :key=\"index\" >\r\n    \r\n           \r\n            <td  scope=\"row2\"><Select2 v-model=\"Item.Product.Id\" :option='this.Products'/></td>\r\n            <td  scope=\"row2\"><Select2 v-model=\"Item.Warehouse.Id\" :option='this.Warehouses'/></td>\r\n            <td  scope=\"row2\"><Select2 v-model=\"Item.Warehouse_cell.Id\" :option='this.Warehouse_cells'/></td>\r\n            <td  scope=\"row2\"><Select2 v-model=\"Item.GTD.Id\" :option='this.GTDs'/></td>\r\n            <td  scope=\"row2\"><input type=\"text\" v-model=\"Item.Quantity\"></td>\r\n            <td  scope=\"row2\"><input type=\"text\" v-model=\"Item.Price\"></td>\r\n            <td  scope=\"row2\"><input type=\"text\" v-model=\"Item.TrustedSignId\"></td>\r\n           \r\n              <!-- delete Button -->\r\n            <td>\r\n                <a href=\"javascript:void(0);\" class=\"link-danger fs-15\"><i class=\"ri-delete-bin-line\" @click=\"RemoveField(index)\"></i></a>\r\n            </td>\r\n        \r\n        </tr>\r\n       \r\n    </tbody>\r\n    <p></p>\r\n    \r\n</table>\r\n\r\n </div>\r\n\r\n    <p>\r\n \r\n    </p>\r\n  \r\n    <div class=\"col-12\">\r\n                  <button class=\"btn btn-success\" type=\"button\" @click=\"addedIncome_Doc\">Added</button>\r\n                 <font color=\"white\">|||</font>\r\n                   <input type=\"button\" value=\"add new row\"  class=\"btn btn-warning\" @click=\"AddField\">\r\n    </div>\r\n         <p>\r\n \r\n    </p>\r\n  \r\n                    \r\n                           \r\n                           \r\n                           \r\n                           \r\n                           \r\n                            </form>\r\n                        </div>\r\n\r\n\r\n\r\n\r\n\r\n    </Layout>\r\n</template>\r\n"]},"metadata":{},"sourceType":"module"}