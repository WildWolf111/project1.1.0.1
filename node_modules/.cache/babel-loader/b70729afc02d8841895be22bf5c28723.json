{"ast":null,"code":"import \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/web.dom-collections.for-each.js\";\nimport \"core-js/modules/es.array.from.js\";\nimport \"core-js/modules/es.string.iterator.js\";\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\nexport default {\n  data: function data() {\n    return {\n      title: \"Placeholders\",\n      items: [{\n        text: \"Base UI\",\n        href: \"/\"\n      }, {\n        text: \"Placeholders\",\n        active: true\n      }]\n    };\n  },\n  name: \"Placeholders\",\n  components: {\n    Layout: Layout,\n    PageHeader: PageHeader\n  },\n  mounted: function mounted() {\n    var checkbox = document.getElementsByClassName(\"code-switcher\");\n    Array.from(checkbox).forEach(function (check) {\n      check.addEventListener('change', function () {\n        var card = check.closest('.card');\n        var preview = card.querySelector(\".live-preview\");\n        var code = card.querySelector(\".code-view\");\n\n        if (check.checked) {\n          // do this\n          preview.classList.add(\"d-none\");\n          code.classList.remove(\"d-none\");\n        } else {\n          // do that\n          preview.classList.remove(\"d-none\");\n          code.classList.add(\"d-none\");\n        }\n      });\n    });\n  }\n};","map":{"version":3,"mappings":";;;;AACA,OAAOA,MAAP,MAAmB,wBAAnB;AACA,OAAOC,UAAP,MAAuB,0BAAvB;AAEA,OAAO,SAAP;AACA,OAAO,0BAAP;AAEA,eAAe;AACXC,MADW,kBACJ;AACH,WAAO;AACHC,WAAK,EAAE,cADJ;AAEHC,WAAK,EAAE,CACH;AACIC,YAAI,EAAE,SADV;AAEIC,YAAI,EAAE;AAFV,OADG,EAKH;AACID,YAAI,EAAE,cADV;AAEIE,cAAM,EAAE;AAFZ,OALG;AAFJ,KAAP;AAaH,GAfU;AAgBXC,MAAI,EAAE,cAhBK;AAiBXC,YAAU,EAAE;AACRT,UAAM,EAANA,MADQ;AAERC,cAAU,EAAVA;AAFQ,GAjBD;AAqBXS,SArBW,qBAqBD;AACN,QAAMC,QAAO,GAAIC,QAAQ,CAACC,sBAAT,CAAgC,eAAhC,CAAjB;AACAC,SAAK,CAACC,IAAN,CAAWJ,QAAX,EAAqBK,OAArB,CAA6B,UAACC,KAAD,EAAW;AACpCA,WAAK,CAACC,gBAAN,CAAuB,QAAvB,EAAiC,YAAM;AACnC,YAAMC,IAAG,GAAIF,KAAK,CAACG,OAAN,CAAc,OAAd,CAAb;AACA,YAAMC,OAAM,GAAIF,IAAI,CAACG,aAAL,CAAmB,eAAnB,CAAhB;AACA,YAAMC,IAAG,GAAIJ,IAAI,CAACG,aAAL,CAAmB,YAAnB,CAAb;;AACA,YAAIL,KAAK,CAACO,OAAV,EAAmB;AACf;AACAH,iBAAO,CAACI,SAAR,CAAkBC,GAAlB,CAAsB,QAAtB;AACAH,cAAI,CAACE,SAAL,CAAeE,MAAf,CAAsB,QAAtB;AACJ,SAJA,MAIO;AACH;AACAN,iBAAO,CAACI,SAAR,CAAkBE,MAAlB,CAAyB,QAAzB;AACAJ,cAAI,CAACE,SAAL,CAAeC,GAAf,CAAmB,QAAnB;AACJ;AACH,OAbD;AAcH,KAfD;AAgBJ;AAvCW,CAAf","names":["Layout","PageHeader","data","title","items","text","href","active","name","components","mounted","checkbox","document","getElementsByClassName","Array","from","forEach","check","addEventListener","card","closest","preview","querySelector","code","checked","classList","add","remove"],"sourceRoot":"","sources":["C:\\Projects\\success\\project_v.1.0.0\\src\\views\\ui\\placeholders.vue"],"sourcesContent":["<script>\nimport Layout from \"../../layouts/main.vue\";\nimport PageHeader from \"@/components/page-header\";\n\nimport 'prismjs';\nimport 'prismjs/themes/prism.css';\n\nexport default {\n    data() {\n        return {\n            title: \"Placeholders\",\n            items: [\n                {\n                    text: \"Base UI\",\n                    href: \"/\",\n                },\n                {\n                    text: \"Placeholders\",\n                    active: true,\n                },\n            ],\n        };\n    },\n    name: \"Placeholders\",\n    components: {\n        Layout,\n        PageHeader,\n    },\n    mounted() {\n        const checkbox = document.getElementsByClassName(\"code-switcher\");\n        Array.from(checkbox).forEach((check) => {\n            check.addEventListener('change', () => {\n                const card = check.closest('.card');\n                const preview = card.querySelector(\".live-preview\");\n                const code = card.querySelector(\".code-view\");\n                if (check.checked) {\n                    // do this\n                    preview.classList.add(\"d-none\");\n                    code.classList.remove(\"d-none\");\n                } else {\n                    // do that\n                    preview.classList.remove(\"d-none\");\n                    code.classList.add(\"d-none\");\n                }\n            });\n        });\n    }\n};\n</script>\n\n<template>\n    <Layout>\n        <PageHeader :title=\"title\" :items=\"items\" />\n        <div class=\"row\">\n            <div class=\"col-lg-12\">\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Default Placeholder</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label\n                                    for=\"baseexample-showcode\"\n                                    class=\"form-label text-muted\"\n                                >Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"baseexample-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n\n                    <div class=\"card-body\">\n                        <p\n                            class=\"text-muted\"\n                        >In the example below, we take a typical card component and recreate it with placeholders applied to create a “loading card”.</p>\n\n                        <div class=\"live-preview\">\n                            <div class=\"row justify-content-center\">\n                                <div class=\"col-xl-7\">\n                                    <div class=\"row justify-content-between\">\n                                        <div class=\"col-lg-5 col-sm-6\">\n                                            <div class=\"card\">\n                                                <img\n                                                    src=\"@/assets/images/small/img-1.jpg\"\n                                                    class=\"card-img-top\"\n                                                    alt=\"card img\"\n                                                />\n\n                                                <div class=\"card-body\">\n                                                    <h5 class=\"card-title\">Card title</h5>\n                                                    <p\n                                                        class=\"card-text\"\n                                                    >Some quick example text to build on the card title and make up the bulk of the card's content.</p>\n                                                    <a href=\"#\" class=\"btn btn-primary\">Go somewhere</a>\n                                                </div>\n                                            </div>\n                                        </div>\n                                        <div class=\"col-lg-5 col-sm-6\">\n                                            <div class=\"card\" aria-hidden=\"true\">\n                                                <img\n                                                    src=\"@/assets/images/small/img-2.jpg\"\n                                                    class=\"card-img-top\"\n                                                    alt=\"card dummy img\"\n                                                />\n                                                <div class=\"card-body\">\n                                                    <h5 class=\"card-title placeholder-glow\">\n                                                        <span class=\"placeholder col-6\"></span>\n                                                    </h5>\n                                                    <p class=\"card-text placeholder-glow\">\n                                                        <span class=\"placeholder col-7\"></span>\n                                                        <span class=\"placeholder col-4\"></span>\n                                                        <span class=\"placeholder col-4\"></span>\n                                                        <span class=\"placeholder col-6\"></span>\n                                                    </p>\n                                                    <a\n                                                        href=\"#\"\n                                                        tabindex=\"-1\"\n                                                        class=\"btn btn-primary disabled placeholder col-6\"\n                                                    ></a>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\" style=\"height: 275px\">\n<code>&lt;!-- Base Examples --&gt;\n&lt;b-card&gt;\n    &lt;img src=&quot;...&quot; class=&quot;card-img-top&quot; alt=&quot;card img&quot;&gt;\n    &lt;b-card no-body&gt;\n        &lt;h5 class=&quot;card-title&quot;&gt;Card title&lt;/h5&gt;\n        &lt;p class=&quot;card-text&quot;&gt;Some quick example text to build on the card title and make up the bulk of the card's content.&lt;/p&gt;\n        &lt;a href=&quot;#&quot; class=&quot;btn btn-primary&quot;&gt;Go somewhere&lt;/a&gt;\n    &lt;/b-card&gt;\n&lt;/b-card&gt;</code>\n\n<code>&lt;b-card aria-hidden=&quot;true&quot;&gt;\n    &lt;img src=&quot;assets/images/small/img-2.jpg&quot; class=&quot;card-img-top&quot; alt=&quot;card dummy img&quot;&gt;\n    &lt;b-card no-body&gt;\n      &lt;b-card title=&quot; &quot; class=&quot;placeholder-glow&quot;&gt;\n        &lt;span class=&quot;placeholder col-6&quot;&gt;&lt;/span&gt;\n      &lt;/b-card&gt;\n      &lt;b-card-text class=&quot;placeholder-glow&quot;&gt;\n        &lt;span class=&quot;placeholder col-7&quot;&gt;&lt;/span&gt;\n        &lt;span class=&quot;placeholder col-4&quot;&gt;&lt;/span&gt;\n        &lt;span class=&quot;placeholder col-4&quot;&gt;&lt;/span&gt;\n        &lt;span class=&quot;placeholder col-6&quot;&gt;&lt;/span&gt;\n      &lt;/b-card-text&gt;\n      &lt;b-link href=&quot;#&quot; tabindex=&quot;-1&quot; class=&quot;btn btn-primary disabled placeholder col-6&quot;&gt;&lt;/b-link&gt;\n    &lt;/b-card&gt;\n&lt;/b-card&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n            </div>\n            <!-- end col -->\n        </div>\n        <!-- end row -->\n\n        <div class=\"row\">\n            <div class=\"col-xxl-6\">\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Width</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"width-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"width-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n                    <div class=\"card-body\">\n                        <p class=\"text-muted\">\n                            Use\n                            <code>w-25,w-50,w-75</code> or\n                            <code>w-100</code> class to placeholder class to set different widths to the placeholder.\n                        </p>\n                        <div class=\"live-preview\">\n                            <span class=\"placeholder w-50\"></span>\n                            <span class=\"placeholder w-75\"></span>\n                            <span class=\"placeholder w-25\"></span>\n                            <span class=\"placeholder w-100\"></span>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Width Sizing--&gt;\n&lt;div class=&quot;live-preview&quot;&gt;\n    &lt;span class=&quot;placeholder col-6&quot;&gt;&lt;/span&gt;\n    &lt;span class=&quot;placeholder w-75&quot;&gt;&lt;/span&gt;\n    &lt;span class=&quot;placeholder&quot; style=&quot;width: 25%;&quot;&gt;&lt;/span&gt;\n&lt;/div&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n            </div>\n            <!-- end col -->\n            <div class=\"col-xxl-6\">\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Sizing</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"sizing-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"sizing-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n                    <div class=\"card-body\">\n                        <p class=\"text-muted\">\n                            Use\n                            <code>placeholder-lg</code>,\n                            <code>placeholder-sm</code>, or\n                            <code>placeholder-xs</code> class to placeholder class to set different size placeholder.\n                        </p>\n                        <div class=\"live-preview\">\n                            <div class=\"row gap-0\">\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder placeholder-lg w-100\"></span>\n                                </div>\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder w-100\"></span>\n                                </div>\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder placeholder-sm w-100\"></span>\n                                </div>\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder placeholder-xs w-100\"></span>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\">\n<code>&lt;!-- Sizing --&gt;\n&lt;span class=&quot;placeholder col-12 placeholder-lg&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 placeholder-sm&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 placeholder-xs&quot;&gt;&lt;/span&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n            </div>\n            <!--end col-->\n        </div>\n        <!-- end row -->\n\n        <div class=\"row\">\n            <div class=\"col-lg-12\">\n                <div class=\"card\">\n                    <div class=\"card-header align-items-center d-flex\">\n                        <h4 class=\"card-title mb-0 flex-grow-1\">Color</h4>\n                        <div class=\"flex-shrink-0\">\n                            <div class=\"form-check form-switch form-switch-right form-switch-md\">\n                                <label for=\"color-showcode\" class=\"form-label text-muted\">Show Code</label>\n                                <input\n                                    class=\"form-check-input code-switcher\"\n                                    type=\"checkbox\"\n                                    id=\"color-showcode\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <!-- end card header -->\n                    <div class=\"card-body\">\n                        <p class=\"text-muted\">\n                            Use\n                            <code>bg-</code> class with the below-mentioned color variation to set a custom color.\n                        </p>\n                        <div class=\"live-preview\">\n                            <div class=\"row g-2\">\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder w-100\"></span>\n                                </div>\n                                <!--end col-->\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder bg-primary w-100\"></span>\n                                </div>\n                                <!--end col-->\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder bg-secondary w-100\"></span>\n                                </div>\n                                <!--end col-->\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder bg-success w-100\"></span>\n                                </div>\n                                <!--end col-->\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder bg-danger w-100\"></span>\n                                </div>\n                                <!--end col-->\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder bg-warning w-100\"></span>\n                                </div>\n                                <!--end col-->\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder bg-info w-100\"></span>\n                                </div>\n                                <!--end col-->\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder bg-light w-100\"></span>\n                                </div>\n                                <!--end col-->\n                                <div class=\"col-12\">\n                                    <span class=\"placeholder bg-dark w-100\"></span>\n                                </div>\n                                <!--end col-->\n                            </div>\n                            <!--end row-->\n                        </div>\n                        <div class=\"d-none code-view\">\n                            <pre class=\"language-markup\" style=\"height: 275px\">\n<code>&lt;!-- Color --&gt;\n&lt;span class=&quot;placeholder col-12 mb-3&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-primary&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-secondary&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-success&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-danger&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-warning&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-info&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-light&quot;&gt;&lt;/span&gt;</code>\n\n<code>&lt;span class=&quot;placeholder col-12 mb-3 bg-dark&quot;&gt;&lt;/span&gt;</code></pre>\n                        </div>\n                    </div>\n                    <!-- end card-body -->\n                </div>\n                <!-- end card -->\n            </div>\n            <!-- end col -->\n        </div>\n        <!--end row-->\n    </Layout>\n</template>"]},"metadata":{},"sourceType":"module"}