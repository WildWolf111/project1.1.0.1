edBarChart,\n      groupedBarChart: groupedBarChart,\n      barImagesChart: barImagesChart,\n    };\n  },\n  components: {\n    Layout,\n    PageHeader\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Basic Bar Chart</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"basicBarChart.series\"\n              :options=\"basicBarChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Custom DataLabels Bar</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"customDatalabelsChart.series\"\n              :options=\"customDatalabelsChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Stacked Bar Chart</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"stackedBarChart.series\"\n              :options=\"stackedBarChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Stacked Bars 100</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"stackedBar100Chart.series\"\n              :options=\"stackedBar100Chart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!--end col-->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"\n              card-header\n              justify-content-between\n              d-flex\n              align-items-center\n            \"\n          >\n            <h4 class=\"card-title\">Bar with Negative Values</h4>\n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"350\"\n              dir=\"ltr\"\n              :series=\"barNegativeChart.series\"\n 