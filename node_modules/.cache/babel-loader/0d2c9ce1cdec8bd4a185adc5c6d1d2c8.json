        return Math.floor(Math.random() * (100 - 1 + 1)) + 1;\n      });\n    },\n\n    reset: function() {\n      this.updatingDonutChart.series = [44, 55, 13, 33];\n    },\n  },\n};\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Simple Pie Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"simplePieChart.series\"\n              :options=\"simplePieChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Simple Donut Chart</h4>\n            \n          </div>\n          <!-- end card header -->\n          <div class=\"card-body\">\n            <apexchart\n              class=\"apex-charts\"\n              height=\"300\"\n              dir=\"ltr\"\n              :series=\"donutChart.series\"\n              :options=\"donutChart.chartOptions\"\n            ></apexchart>\n          </div>\n        </div>\n        <!--end card-->\n      </div>\n      <!-- end col -->\n    </div>\n    <!-- end row -->\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div\n            class=\"card-header justify-content-between d-flex align-items-center\"\n          >\n            <h4 class=\"card-title\">Updating Donut Chart</h4>\n            \n          </di